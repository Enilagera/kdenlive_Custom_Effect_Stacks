<effectgroup description="LUT base effect stack." id="LM Creative - LUT" parentIn="0">
 <description>LUT base effect stack.</description>
 <effect id="avfilter.eq">
  <property name="av.gamma_weight">1</property>
  <property name="av.gamma_g">1</property>
  <property name="av.gamma_b">1</property>
  <property name="av.gamma_r">1</property>
  <property name="av.gamma">1</property>
  <property name="av.saturation">1</property>
  <property name="av.brightness">0</property>
  <property name="av.contrast">1</property>
 </effect>
 <effect id="mask_start-frei0r.select0r">
  <property name="filter">frei0r.select0r</property>
  <property name="filter.Delta B / I / I">0=0.2</property>
  <property name="filter.Delta G / B / Chroma">0=0.2</property>
  <property name="filter.Edge mode">0.9</property>
  <property name="filter.Subspace shape">0.5</property>
  <property name="filter.Selection subspace">0</property>
  <property name="filter.Delta R / A / Hue">0=0.2</property>
  <property name="filter.Invert selection">0</property>
  <property name="filter.Operation">0.7</property>
  <property name="filter.Slope">0=0</property>
  <property name="filter.Color to select">0=</property>
 </effect>
 <effect id="frei0r.transparency">
  <property name="0">0=1</property>
 </effect>
 <effect id="avfilter.lut3d">
  <property name="av.interp">tetrahedral</property>
  <property name="av.file">/app/share/kdenlive/luts/CINEMATIC.cube</property>
 </effect>
 <effect id="mask_apply"/>
</effectgroup>
