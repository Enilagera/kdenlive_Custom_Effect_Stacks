  <property name="av.prev">0</property>
  <property name="av.overlap">0.5</property>
  <property name="av.block">4</property>
  <property name="av.amount">0.01</property>
  <property name="av.sigma">1</property>
 </effect>
 <effect id="avfilter.unsharp">
  <property name="av.ca">0</property>
  <property name="av.cy">5</property>
  <property name="av.cx">5</property>
  <property name="av.la">0</property>
  <property name="av.ly">5</property>
  <property name="av.lx">5</property>
 </effect>
 <effect id="avfilter.noise">
  <property name="av.c3f">a</property>
  <property name="av.all_seed">-1</property>
  <property name="av.c2f">a</property>
  <property name="av.c0_seed">-1</property>
  <property name="av.c1_seed">-1</property>
  <property name="av.c2_seed">-1</property>
  <property name="av.alls">0</property>
