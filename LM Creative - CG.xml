  <property name="av.c1f">a</property>
  <property name="av.c2s">0</property>
  <property name="av.c3s">0</property>
 </effect>
 <effect id="avfilter.vibrance">
  <property name="av.alternate">0</property>
  <property name="av.blum">0</property>
  <property name="av.bbal">1</property>
  <property name="av.glum">0</property>
  <property name="av.gbal">1</property>
  <property name="av.rlum">0</property>
  <property name="av.rbal">1</property>
  <property name="av.intensity">0</property>
 </effect>
 <effect id="frei0r.saturat0r">
  <property name="Saturation">0=0.125</property>
