  <property name="gain_g">0=1</property>
  <property name="gain_b">0=1</property>
  <property name="gamma_g">0=1</property>
  <property name="gamma_r">0=1</property>
  <property name="gain_r">0=1</property>
  <property name="lift_b">0=0</property>
  <property name="gamma_b">0=1</property>
  <property name="lift_g">0=0</property>
  <property name="lift_r">0=0</property>
 </effect>
 <effect id="avfilter.lut3d">
  <property name="av.interp">tetrahedral</property>
  <property name="av.file">/app/share/kdenlive/luts/TEAL_ORANGE.cube</property>
