<effectgroup description="Simple zoom out transition with lens destortion. Cut 12 frames to apply effect." id="Zoom Out Transition 12f" parentIn="0">
 <description>Simple zoom out transition with lens destortion. Cut 12 frames to apply effect.</description>
 <effect id="pan_zoom">
  <property name="transition.repeat_off">1</property>
  <property name="producer.resource">0x00000000</property>
  <property name="use_normalised">1</property>
  <property name="transition.distort">1</property>
  <property name="transition.mirror_off">1</property>
  <property name="transition.rect">0h=-2880 -1620 7680 4320;11=0 0 1920 1080</property>
 </effect>
 <effect id="frei0r.lenscorrection">
  <property name="brightness">0h=0;11=0</property>
  <property name="correctionnearedges">0h=0.194;11=0.5</property>
  <property name="correctionnearcenter">0h=0.435;11=0.5</property>
  <property name="ycenter">0h=0.5;11=0.5</property>
  <property name="xcenter">0h=0.5;11=0.5</property>
 </effect>
 <effect id="avfilter.gblur">
  <property name="av.sigmaV">0h=20;11=0</property>
  <property name="kdenlive:collapsed">1</property>
  <property name="av.planes">7</property>
  <property name="av.steps">2</property>
  <property name="av.sigma">0h=20;11=0</property>
 </effect>
</effectgroup>
